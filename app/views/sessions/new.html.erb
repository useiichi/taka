<div class="mt-3">
<h2>Log in</h2>
</div>
  
<%= form_tag( :controller => "sessions", :action => "create" ) do %>  
  <div class="field">  
    <%= label_tag :password %>  
    <%= password_field_tag :password %>  
  </div>  
  <p>パスワードはあなたの名前です。</p>
  <div class="actions"><%= submit_tag "Log in" %></div>  
<% end %>
